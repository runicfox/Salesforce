        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Account.AccPac_Account__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <description>Set to customer when AccPac number is added.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
